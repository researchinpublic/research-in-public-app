{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "SessionResponse",
  "type": "object",
  "properties": {
    "session_id": {
      "type": "string",
      "format": "uuid"
    },
    "user_id": {
      "type": "string"
    },
    "created_at": {
      "type": "string",
      "format": "date-time"
    },
    "message_count": {
      "type": "integer",
      "minimum": 0
    }
  },
  "required": ["session_id", "user_id", "created_at"]
}

